{
  "private": true,
  "description": "An example project for Auth.js with SvelteKit",
  "repository": "https://github.com/nextauthjs/sveltekit-auth-example",
  "bugs": {
    "url": "https://github.com/nextauthjs/next-auth/issues"
  },
  "homepage": "https://sveltekit-auth-example.vercel.app",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "postinstall": "prisma generate"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "latest",
    "@sveltejs/adapter-vercel": "latest",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "prisma": "^5.1.0",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-loading-spinners": "^0.3.4",
    "svelte-typeahead": "^4.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@auth/core": "latest",
    "@auth/sveltekit": "latest",
    "@fontsource/fira-mono": "^4.5.0",
    "@prisma/client": "^5.1.0",
    "@sveltejs/adapter-node": "^2.0.0",
    "@vercel/analytics": "^1.1.2",
    "ag-grid-community": "^28.2.1",
    "dotenv": "^16.1.4",
    "svelte-web3": "^4.0.0"
  },
  "type": "module"
}
